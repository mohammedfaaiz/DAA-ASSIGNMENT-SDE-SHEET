#include <bits/stdc++.h>
using namespace std; 
vector<vector<int>> pairSum(vector<int> &arr, int s){
 vector<vector<int>> ans;
   sort(arr.begin(),arr.end());
   int i=0; 
  int j=i+1; 
  while(i<arr.size() && j<arr.size()){ 
     if((arr[i]+arr[j])==s){  
       ans.push_back({arr[i],arr[j]});  
       if(j==arr.size()-1){ 
        i++;   
      j=i+1;    
  }     
 else{     
    j++;
    }
     }    
  else if((arr[i]+arr[j])>s){   
      i++;   
      j=i+1;    
  }    
  else if((arr[i]+arr[j])<s){   
          if(j==arr.size()-1){ 
        i++;         
        j=i+1;   
   }    
  else{ 
  j++;     
      } 
   }  
 }   
return ans;    
}
