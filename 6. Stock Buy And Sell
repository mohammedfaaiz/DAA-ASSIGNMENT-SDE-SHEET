#include <bits/stdc++.h> 
int maximumProfit(vector<int> &prices){
   int todaysProfit=0, totalProfit=0, buyingMinute=0;
   int n=prices.size()-1;
   for(int i=0; i<=n; i++) {
       int todaysProfit= prices[i]-prices[buyingMinute];
       if(todaysProfit <= 0) buyingMinute=i;
       if(todaysProfit > totalProfit) totalProfit=todaysProfit;
   }
   return totalProfit;
}
