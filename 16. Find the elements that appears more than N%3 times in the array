#include <bits/stdc++.h>
vector<int> majorityElementII(vector<int> &arr)
{
    vector<int> ans;
    int n=arr.size();
    int m=n/3;
    unordered_map<int, int>mp;
    for(int i=0; i<n; i++){
        mp[arr[i]]++;
    }
    for(auto ele:mp){
        if(ele.second>m){
            ans.push_back(ele.first);
        }
    }
    return ans;
}
